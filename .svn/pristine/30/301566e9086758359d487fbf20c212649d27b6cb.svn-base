}


#footer .footer_main .icp_no {
	line-height: 64px;
	color: #aaa;
	font-size: 12px;
	position: relative;
	text-align: center;
}

#footer .footer_main .icp_no .Trust_Website_Seal {
	width: 95px;
	height: 35px;
	position: absolute;
	right: 0;
	top: 20px;

}

#footer .footer_main .icp_no .Trust_Website_Seal img {
	width: 100%;
	height: 100%;
