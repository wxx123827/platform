}


ul,li{
	list-style: none;
}

.content_box{
	width: 285px;
	height: 350px;
	text-align: center;
	background-color: #ffffff;
	margin-bottom: 10px;
	float: left;
	border: 1px solid #eeeeee;
	box-sizing: border-box;
}
.content_box img{
	width: 265px;
    height: 200px;
    margin: 10px;
    box-sizing: border-box;
    object-fit: scale-down;
}
.content_box ul{
	padding-left: 0px;
	padding-top: 5px;
	box-sizing: border-box;
}
.content_box li{
	padding-top: 8px;
	box-sizing: border-box;
	color: #666666;
	width: 250px;
	margin: 0 auto;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.content_box li:first-child{
	color: #333333;
	font-size: 14px;
}
.content_box li:nth-child(2){
	color: #666666;
	font-size: 12px;
}
.content_box li:hover{
    color: #247dd0;
}
.content_box:nth-child(4n){
    margin-right: 0;
}
.content_box:hover{
	cursor: pointer;
    box-shadow: 0px 3px 15px 0px 
        rgba(0, 0, 0, 0.35);
}
#nav_type {
    height: 38px;
    width: 100%;
	min-width: 1200px;
	border-bottom: 3px solid #247dd0;
	background-color: #fff;
    position: relative;
    z-index: 6;
}


#nav_type .nav {
    /* padding: 0 18px; */
    height: 38px;
    width: 83.3333%;
    white-space: nowrap;
}

#nav_type .nav li {
    list-style: none;
}

#nav_type .nav li.fl div.nav_item {
    padding: 11px;
    margin-left: 23px;
    position: relative;
}

#nav_type .nav li.fl div.nav_item>a,
#nav_type .nav li.fl div.nav_item>a:visited {
    display: block;
    padding: 0 18px;
    font-size: 16px;
    line-height: 1;
    font-family: 'SourceHanSansCN-Bold';
    color: #333;
    cursor: pointer;
}

#nav_type .nav li.fl div.nav_item>a.bold,
#nav_type .nav li.fl div.nav_item>a.active,
#nav_type .nav li.fl div.nav_item>a:visited.bold {
    font-weight: bold;
}
#all_types {
    width: 16.66666667%;
    text-align: center;
    line-height: 38px;
    background-color: #247dd0;
    color: #ffffff;
    font-size: 14px;
    cursor: pointer;
}
.container {
    width: 1200px;
    margin: 0 auto;
    clear: both;
}


