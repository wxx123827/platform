body{
  width: 100%;
  min-width: 1200px;
  background-color: #f9f9f9;
}
#front_header {
	width: 100%;
	height: 40px;
	font-family: 'SourceHanSansCN-Normal';
	border: solid 1px #bfbfbf;
	line-height: 40px;
	font-size: 12px;
	color: #666;
	box-sizing: border-box;
}

#front_header:after {
	display: block;
	content: '';
	clear: both;
	height: 0;
}

.head_container {
	width: 1200px;
	margin: 0 auto;
	clear: both;
}

.right {
	float: right;
}

#front_header .right a {
	margin-left: 90px;
}

.left {
	float: left;
}

.about {
	margin-left: 100px;
}

/* 搜索框 */
#search_container {
	width:100%;
	height: 123px;
	background: #fff;
	min-width: 1200px;
	padding-top: 40px;
	padding-bottom: 30px;
	box-sizing: border-box;
    z-index: 99;
}
.searchcontainer{
	    width: 1200px;
	    margin: 0 auto;
	    clear: both;
}

#search_container.active {
	position: fixed;
	padding: 0;
	z-index: 20;
	top: 0;
	-webkit-box-shadow: 0 1px 4px #ddd;
	-moz-box-shadow: 0 1px 4px #ddd;
	box-shadow: 0 1px 4px #ddd;
	-webkit-transition: top 0.3s ease-in-out 0.3s;
	-moz-transition: top 0.3s ease-in-out 0.3s;
	-ms-transition: top 0.3s ease-in-out 0.3s;
	-o-transition: top 0.3s ease-in-out 0.3s;
	transition: top 0.3s ease-in-out 0.3s;
}

#search_container .logo {
	width: 18%;
	height: 60px;
}

#search_container .logo img {
	width: 200px;
	float: left;
}

#search_container .punlish_container {

	position: relative;

	width: 200px;
	height: 45px;

}

#search_container .punlish_container .publish {
	height: 45px;
	width: 100%;
	border: solid 1px #247dd0;
	border-radius: 5px;
	color: #247dd0;
	cursor: pointer;
	background-color: #fff;
	font-size: 14px;
}

#search_container .punlish_container .publish:hover {
	font-size: 17px;
}
#search {
	width: 62%;
	border: solid 3px #247dd0;
	height: 45px;
  
	position: relative;
	font-size: 12px;
  }
  
  #search dl {
	width: 13.16949153%;
	cursor: pointer;
	text-align: center;
	position: relative;
	z-index: 17;
	font-size: 16px;
	color: #333333;
	height: 45px;
	float: left;
	border-right: none;
  }
  
  #search dl dt {
	line-height: 45px;
	width: 100%;
	font-size: 15px;
	margin-right: 12px;
	display: inline-block;
	background: url(../../imgs/down.png) right center no-repeat;
  }
  
  #search dl dt:hover {
	background: url(../../imgs/up.png) right center no-repeat;
  }
  
  
  #search dl dd.all:before {
	position: absolute;
	top: -6px;
	left: 50%;
	margin-left: -4px;
	content: '';
	display: inline-block;
	height: 8px;
	width: 8px;
	border-top: 1px solid #ddd;
	border-left: 1px solid #ddd;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
	background: #fff;
  }
  
  #search dl dd,
  #search dl dd:visited {
	background: #fff;
	position: relative;
	z-index: 1;
	line-height: 34px;
	visibility: hidden;
	padding: 0 8px;
	border: 1px solid #ddd;
	border-bottom: none;
  }
  
  #search dl dd.last {
	border-bottom: 1px solid #ddd;
  }
  
  #search dl dd:hover {
	background: #eee;
  
  }
  
  #search dl dd:hover:before {
	background: #eee;
  }
  
  #search .key_words {
	width: 68%;
	height: 31px;
	line-height: 38px;
	padding: 7px;
	margin-left: 10px;
	border: none;
	font-size: 14px;
	outline: none;
  }
  
  #search .key_words.all {
	width: 78.98305085%;
  }
  
  #search .go_search {
	width: 15.01694915%;
	height: 100%;
	font-size: 15px;
	padding-top: -5px;
	border: none;
	background-color: #247dd0;
	color: #fefefe;
	cursor: pointer;
	letter-spacing: 6px;
	text-indent: 6px;
  }
  
  #search .go_search:hover {
	font-size: 16px;
  }
  
  #search #hotWords {
	top: 55px;
	position: absolute;
	left: 0;
	bottom: 20px;
  }
  
  #search #hotWords a,
  #search #hotWords a:visited {
	color: #999;
	font-size: 12px;
	padding-right: 8px;
	border-right: 1px solid #999999;
	margin-right: 8px;
	line-height: 12px;
	float: left;
  }
  
  #search #hotWords a:last-child{
	  border-right: none;
  }
  #search #hotWords a:hover,
  #search #hotWords a:visited:hover {
	font-family: 'SourceHanSansCN-Regular';
	font-size: 12px;
  
	color: #247dd0;
  }

.tab_nav {
    width: 200px;
	height: 160px;
	text-align: center;
	box-sizing: border-box;
    position: absolute;
    top: 48px;
	z-index: 200000;
	background-color: #fff;
}

.tab_nav a {
    width: 200px;
    display: block;
	height: 30px;
	line-height: 30px;
	color: #247dd0;
	font-size: 14px;
    box-sizing: border-box;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
#nav_box{
	height: 50px;
}
#footer {
	width: 100%;
	clear: both;
	height: 350px;
	background-color: #272525;

}

#footer .footer_main {
	width: 1200px;
	margin: 0 auto;
}

#footer .footer_main .ykx_intro {
	padding: 30px 0 20px 0px;
	border-bottom: 1px solid #999999;
}

#footer .footer_main .ykx_intro .footer_img {
	border-bottom: 1px solid #999999;
	padding-left: 190px;
	padding-bottom: 10px;
}

#footer .footer_main .ykx_intro:after {
	display: block;
	height: 0;
	content: '';
	clear: both;
}

#footer .footer_main .ykx_intro dl {
	float: left;
	margin-left: 200px;

	position: relative;
}

#footer .footer_main .ykx_intro dl dt {
	font-size: 14px;
	color: #eeeeee;
	font-weight: 400;
	margin-top: 15px;
	margin-bottom: 15px;
}

#footer .footer_main .ykx_intro dl dd {
	line-height: 25px;
	font-size: 12px;
	color: #999999;
}


#footer .footer_main .ykx_intro dl dd a {
	color: #999999;
}

#footer .footer_main .ykx_intro dl dd a:hover {
