body{
	background-color: #f9f9f9;
}
@font-face {
	font-family: 'SourceHanSansCN-Ragular';
	src: url('../font/SourceHanSansCN-Ragular.otf');
	src: url('../font/SourceHanSansCN-Ragular.otf') format('otf');
	font-family: 'SourceHanSansCN-Bold';
	src: url('../font/SourceHanSansCN-Bold.otf');
	src: url('../font/SourceHanSansCN-Bold.otf') format('otf');
	font-weight: normal;
	font-style: normal;
}

li {
	list-style: none;
}
a{
	text-decoration: none;
}
#search #hotWords a:last-child {
	border-right: none;
}

.nav_middle {
	width: 1200px;
	height: 1600px;
	margin: 0 auto;
}

 #nav_type .nav li {
   list-style: none;
 }

 #nav_type .nav li.fl div.nav_item>a,
 #nav_type .nav li.fl div.nav_item>a:visited {
   display: block;
   padding: 0 18px;
   font-size: 16px;
   line-height: 1;
   font-family: 'SourceHanSansCN-Bold';
   color: #333;
   cursor: pointer;
   text-decoration: none;
 }


 #nav_type .nav li.fl div.nav_item>a.border,
 #nav_type .nav li.fl div.nav_item>a:visited.border {
   border-right: 1px solid #ddd;
 }

 #nav_type .nav li.fl div.nav_item>a.active,
 #nav_type .nav li.fl div.nav_item>a:visited.active {
   color: #247dd0;
 }

 #nav_type .nav li.fl div.nav_item>a:hover:not(.active),
 #nav_type .nav li.fl div.nav_item>a:visited:hover:not(.active) {
   color: #247dd0;
 }
  .fl {
    float: left;
  }

.container {
	padding-left: 0;
}
#all_types:before {
	width: 16px;
	height: 16px;
	/* content: ''; */
	display: inline-block;
	position: relative;
	top: -2px;
	margin-right: 12px;
	vertical-align: middle;

}

.all_typetext {
	font-family: 'SourceHanSansCN-Regular';
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #666666;
	padding-left: 10px;
	box-sizing: border-box;
}

.shiboqi {
	font-family: 'SourceHanSansCN-Regular';
	font-size: 12px;
	font-weight: normal;
	letter-spacing: 0px;
	color: #e31f1f;
	padding-left: 5px;
	box-sizing: border-box;
}

.all_types {
	background-color: #ffffff;
	padding-bottom: 20px;
}

.brand {
	width: 1200px;
	height: 40px;
	border: solid 1px #a0a0a0;
	background-color: #ffffff;
}

.region {
	width: 1200px;
	height: 40px;
	border: solid 1px #a0a0a0;
	border-top: none;
	background-color: #ffffff;
}

.brand_title,
.region_title {
	display: inline-block;
	width: 110px;
	height: 38px;
	line-height: 38px;
	text-align: center;
	background-color: #eeeeee;
	border-right: 1px solid #a0a0a0;
}

.region_title {
	height: 39px;
	line-height: 39px;
}

.brand input {
	margin-left: 20px;
}

.region input {
	margin-left: 20px;
}
#search {
	width: 62%;
	border: solid 3px #247dd0;
	height: 45px;

	position: relative;
	font-size: 12px;
}

#search dl {
	width: 13.16949153%;
	cursor: pointer;
	text-align: center;
	position: relative;
	z-index: 17;
	font-size: 16px;
	color: #333333;
	height: 45px;
	float: left;
	border-right: none;
}

#search dl dt {
	line-height: 40px;
	/* margin-top: -5px; */
	width: 100%;
	font-size: 15px;
	margin-right: 12px;
	display: inline-block;
	background: url('../imgs/down.png') right center no-repeat;
}

#search dl dt:hover {

	background: url('../imgs/up.png') right center no-repeat;
}


#search dl dd.all:before {
	position: absolute;
	top: -6px;
	left: 50%;
	margin-left: -4px;
	content: '';
	display: inline-block;
	height: 8px;
	width: 8px;
	border-top: 1px solid #ddd;
	border-left: 1px solid #ddd;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
	background: #fff;
}

#search dl dd,
#search dl dd:visited {
	background: #fff;
	position: relative;
	z-index: 1;
	line-height: 34px;
	visibility: hidden;
	padding: 0 8px;
	border: 1px solid #ddd;
	border-bottom: none;
}

#search dl dd.last {
	border-bottom: 1px solid #ddd;
}

#search dl dd:hover {
	background: #eee;

}

#search dl dd:hover:before {
	background: #eee;
}

#search .key_words {
	width: 70%;
	height: 38px;
	line-height: 45px;
	padding: 7px;
	margin-left: 10px;
	border: none;
	font-size: 14px;
	outline: none;
}

#search .key_words.all {
	width: 78.98305085%;
}

#search .go_search {
	width: 15.01694915%;
	height: 100%;
	padding-top: -5px;
	border: none;
	background-color: #247dd0;
	color: #fefefe;
	cursor: pointer;
	letter-spacing: 6px;
	text-indent: 6px;
}

#search .go_search:hover {
	font-size: 16px;
}

#search #hotWords {
	top: 55px;
	position: absolute;
	left: 0;
	bottom: 20px;
}

#search #hotWords a,
#search #hotWords a:visited {
	color: #999;
	font-size: 12px;
	padding-right: 8px;
	border-right: 1px solid #999999;
	margin-right: 8px;
	line-height: 12px;
	float: left;
}

