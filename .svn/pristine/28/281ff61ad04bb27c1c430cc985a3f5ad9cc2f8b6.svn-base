.basic_content {
    clear: both;
    width: 100%;
}

.content {
    width: 1200px;
    margin: 0 auto;
}

.content .left{
    width: 190px;
    height: 456px;
    background-color: #ffffff;
    text-align: center;
}
.content .left ul{
    margin-top: 20px;
}
.content .left li a{
    font-size: 16px;
    display: inline-block;
    width: 190px;
    height: 35px;
    line-height: 35px;
}
.content .left li{
    width: 190px;
    height: 35px;
    line-height: 35px;
}
.content .left li:hover{
    width: 190px;
    height: 35px;
    background-color: #eeeeee;
}

.content .right {
    width: 1000px;
    background-color: #ffffff;
    padding-bottom: 120px;
}

.content .right h2{
    font-size: 20px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #333333;
    padding: 20px 0px 10px 20px;
}