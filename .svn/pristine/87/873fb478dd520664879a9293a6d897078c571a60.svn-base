*{
    margin: 0;
    padding: 0;
}
html,body{
    width: 100%;
  
    background-color:#f9f9f9;
}
@font-face {
    font-family: 'SourceHanSansCN-Regular';
    src: url('../font/SourceHanSansCN-Regular.otf');
    src: url('../font/SourceHanSansCN-Regular.otf') format('otf');
    font-weight: normal;
    font-style: normal;
  }
.container {
    width: 1200px;
    margin: 0 auto;
    clear: both;
  }
  .fl {
    float: left;
  }
  .index{
      margin-left: 100px;
  }
  .fr {
    float: right;
  }
 #front_header .fr a{
      margin-left: 90px;
  }
  .clear,
  .clearfix {
    clear: both;
  }
  .business{
	  float: left;
	  margin-left: 0px;
  }
  .otherImg{
	  float:left;
	  margin-left: 0px!important;
  }
  
  
  a{
    text-decoration: none;
    color: #666;
  }
  a:link,a:visited,a:active{
    text-decoration: none;
  }
.main{
    width: 1200px;
    margin: 0 auto;
}
.main_content_top{
    height: 165px;
    background-color:#ffffff;
   
}
.main_content_top p{
    
	height: 18px;
	font-family: 'SourceHanSansCN-Regular';
	font-size: 18px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #333333;
    padding-left: 210px;
    padding-top: 65px;
    box-sizing: border-box;
}
.main_content_bottom{
    background-color:#ffffff;
    margin-top: 20px;
}
.circle div{
    float: left;
    width: 90px;
    height: 130px;
    margin-top: -60px;
}
.circle div a{
    display: block;
    width: 90px;
    height: 90px;
    line-height: 90px;
    border-radius: 50%;
    text-align: center;
    text-decoration: none;
    color: #fff;
    background-color: #999999;
}
.circle div p{
    text-align: center;
    margin-top: 15px;
    font-size: 16px;
}
.square{
    clear: both;
 
}
.square div{
    width: 200px;
    height: 160px;
    border-radius: 5px;
    border: 1px solid #ccc;
    float: left;
    text-align: center;
    margin-left: 201px;
    margin-top: 190px;
}
.square div a{
    text-decoration: none;
    line-height: 160px;
    color: #999999;
    font-size: 16px;
   
}
.square div:hover{
    background-color: #247dd0;
    color: #fff;
}
.square div:hover a{
  color: #fff;
}
.square div:first-child{
    margin-left: 90px;
}




#sorts {
  width: 200px;
  background-color: #ffffff;
  height: 420px;
}

#sorts li {
  list-style: none;
}

#search {
  width: 62%;
  border: solid 3px #247dd0;
  height: 45px;

  position: relative;
  font-size: 12px;
}

#search dl {
  width: 13.16949153%;
  cursor: pointer;
  text-align: center;
  position: relative;
  z-index: 17;
  font-size: 16px;
  color: #333333;
  height: 45px;
  float: left;
  border-right: none;
}

#search dl dt {
  line-height: 50px;
  /* margin-top: -5px; */
  width: 100%;
  font-size: 15px;
  margin-right: 12px;
  display: inline-block;
  background: url('../imgs/down.png') right center no-repeat;
}

#search dl dt:hover {

  background: url('../imgs/up.png') right center no-repeat;
}


#search dl dd.all:before {
  position: absolute;
  top: -6px;
  left: 50%;
  margin-left: -4px;
  content: '';
  display: inline-block;
  height: 8px;
  width: 8px;
  border-top: 1px solid #ddd;
  border-left: 1px solid #ddd;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
  background: #fff;
}

#search dl dd,
#search dl dd:visited {
  background: #fff;
  position: relative;
  z-index: 1;
  line-height: 34px;
  visibility: hidden;
  padding: 0 8px;
  border: 1px solid #ddd;
  border-bottom: none;
}

#search dl dd.last {
  border-bottom: 1px solid #ddd;
}

#search dl dd:hover {
  background: #eee;

}

#search dl dd:hover:before {
  background: #eee;
}

#search .key_words {
  width: 60.81355932%;
  height: 31px;
  line-height: 45px;
  padding: 7px;
  margin-left: 10px;
  border: none;
  font-size: 14px;
  outline: none;
}

#search .key_words.all {
  width: 78.98305085%;
}

#search .go_search {
  width: 15.01694915%;
  height: 100%;
  padding-top: -5px;
  border: none;
  background-color: #247dd0;
  color: #fefefe;
  cursor: pointer;
  letter-spacing: 6px;
  text-indent: 6px;
}

#search .go_search:hover {
  font-size: 16px;
}

#search #hotWords {
  top: 55px;
  position: absolute;
  left: 0;
  bottom: 20px;
}

#search #hotWords a,
#search #hotWords a:visited {
  color: #999;
  font-size: 12px;
  padding-right: 8px;
  border-right: 1px solid #999999;
  margin-right: 8px;
  line-height: 12px;
  float: left;
}

#search #hotWords a:hover,
#search #hotWords a:visited:hover {
  font-family: 'SourceHanSansCN-Regular';
  font-size: 12px;

  color: #247dd0;
}




