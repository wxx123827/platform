<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>实验室共享</title>
    <link rel="stylesheet" href="css/common/headandfoot.css" type="text/css" />
    <link rel="stylesheet" href="css/share.css" type="text/css" />
    <link rel="stylesheet" href="layui/css/layui.css" media="all">
    <link rel="stylesheet" href="css/common/classify.css" type="text/css" />
</head>
<style>
    #search dl dt {
        line-height: 38px !important;
    }

    #search .key_words {
        height: 38px !important;
        width: 70% !important;
    }

    .brand {
        margin-top: 20px;
    }

    .layui-carousel {
        height: 420px !important;
    }

    .layui-carousel>[carousel-item] {
        height: 420px !important;
    }

    .layui-carousel>[carousel-item]>* {
        height: 420px !important;
    }

    .layui-carousel>[carousel-item]>.layui-this {
        background-color: #efefef;
    }
</style>

<body style="position: relative;">

    <!--顶部-->
    <div id="front_header" class="head"></div>
    <!--搜索栏-->
    <div id="search_container" class="search"></div>
    <!--分类导航-->
    <div id="nav_type">
        <div class="container" style="position: relative">
            <div class="classify">
                <p id="all_types" class="fl types">
                    <img src="imgs/nav_more.png">
                    <span>实验室共享</span>
                    <p class="fl" id="classify"
                        style="display: none;position: absolute;left: 0px;top: 38px;z-index: 99;">
                    </p>
                </p>
            </div>

            <ul class="fl nav" id="drop_down_nav">
                <li class="trangel" style="left: 568.5px; display: none;"></li>
                <li class="fl">
                    <div class="nav_item">
                        <a class="active" href="index.html">首页</a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!--banner图及分类-->
    <div class="nav_middle">
        <div id="banner_types" class="container">
            <div class="fr" id="banners">
                <div class="fl carousel_container">
                    <div class="main_banner">
                        <div id="myCarousel" class="layui-carousel">
                            <div carousel-item class="banner_list">
                            </div>
                        </div>

                    </div>

                </div>

            </div>
        </div>
    </div>
    <!-- 商品类别 -->
   <div class="goods_detail">
   </div>

    <!-- 底部 -->
    <div id="footer" class="foot" style="clear:both;">
    </div>

    <script src="js/jquery.min.js"></script>
    <script src="layui/layui.js"></script>
    <script src="js/baseconstant.js"></script>
    <script src="js/classify-common.js"></script>
    <script src="js/share.js"></script>
</body>

</html>