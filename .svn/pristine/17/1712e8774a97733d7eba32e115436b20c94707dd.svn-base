/*toubu*/
a {
    text-decoration: none;
    color: #666666;
}

body,
html,
div,ul,li {
    margin: 0;
    padding: 0;
}

body {
    width: 100%;
    background-color: #f9f9f9;
}

#front_header {
    width: 100%;
    height: 40px;
    font-family: 'SourceHanSansCN-Normal';
    border: solid 1px #bfbfbf;
    line-height: 40px;
    font-size: 14px;
    color: #666;
}

#front_header:after {
    display: block;
    content: '';
    clear: both;
    height: 0;

}
.basic_head{
    width: 100%;
    height: 40px;
    line-height: 40px;
	border: solid 1px #bfbfbf;
}
.head_container {
    width: 1200px;
    margin: 0 auto;
    clear: both;
    height: 40px;
    line-height: 40px;
}

.right {
    float: right;
}

.head_container .right a {
    margin-left: 50px;
}

.head_container .right a img {
    margin-right: 5px;
}

.left {
    float: left;
}

.about {
    margin-left: 100px;
}

/* logo部分 */
.center {
    clear: both;
    width: 100%;
    height: 100px;
    line-height: 100px;
    background-color: #247dd0;
}

.personal_center {
    width: 1200px;
    margin: 0 auto;
}
.center_middle {
    margin-left: 10px;
    height: 100%;
    width: 200px;
    border-left: 1px solid #ffffff;

}
.center_middle span{
    font-size: 22px;
    color: #ffffff;
    padding-left: 20px;
    font-weight: normal;
}
.center_right a {
    width: 120px;
    height: 40px;
    line-height: 40px;
    background-color: #ffffff;
    border-radius: 5px;
    color: #247dd0;
}

/* 公共底部 */
#footer {
    width: 100%;
    clear: both;
    /* height: 350px; */
    background-color: #272525;

}

#footer .footer_main {
    width: 1200px;
    margin: 0 auto;
}

#footer .footer_main .ykx_intro {
    padding: 30px 0 20px 0px;
    border-bottom: 1px solid #999999;
}

#footer .footer_main .ykx_intro .footer_img {
    border-bottom: 1px solid #999999;
    padding-left: 190px;
    padding-bottom: 10px;
}

#footer .footer_main .ykx_intro:after {
    display: block;
    height: 0;
    content: '';
    clear: both;
}

#footer .footer_main .ykx_intro dl {
    float: left;
    margin-left: 200px;

    position: relative;
}

#footer .footer_main .ykx_intro dl dt {
    font-size: 16px;
    color: #eeeeee;
    font-weight: 400;
    margin-top: 15px;
    margin-bottom: 15px;
}

#footer .footer_main .ykx_intro dl dd {
    line-height: 25px;
    font-size: 14px;
    color: #999999;
    margin-left: 0px;

}


#footer .footer_main .ykx_intro dl dd a {
    color: #999999;
}

#footer .footer_main .ykx_intro dl dd a:hover {
    color: #e31f1f;
}


#footer .footer_main .icp_no {
    line-height: 64px;
    color: #aaa;
    font-size: 14px;
    position: relative;
    text-align: center;
}

#footer .footer_main .icp_no .Trust_Website_Seal {
    width: 95px;
    height: 35px;
    position: absolute;
    right: 0;
    top: 20px;

}

#footer .footer_main .icp_no .Trust_Website_Seal img {
    width: 100%;
    height: 100%;
    vertical-align: top;
    margin-top: -4px;
}

ul,li{
    list-style: none;
}
