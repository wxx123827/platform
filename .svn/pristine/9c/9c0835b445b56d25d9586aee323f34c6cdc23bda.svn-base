* {
  margin: 0;
  padding: 0;
}
@font-face {
  font-family: 'SourceHanSansCN-Regular';
  src: url('../font/SourceHanSansCN-Regular.otf');
  src: url('../font/SourceHanSansCN-Regular.otf') format('otf');
  font-weight: normal;
  font-style: normal;
}
div,
h2,
input,
p {
  margin: 0;
  padding: 0;
}
html,
body {
  height: 100%;
  width: 100%;
  min-width: 1200px;
}
body {
  background-size: 100% 100%;
}
.main {
  width: 100%;
  height: 100%;
  position: relative;
}
.main .main_top {
  width: 100%;
  height: 100px;
}
.main .main_top img{
  margin-top: 0.183rem;
  margin-bottom: 1rem;  
  margin-left: 3rem;
  width: 200px;
  height: 56px;
  float: left;
}
.main .main_middle{
  width: 100%;
  height: 600px;
  background-color: #0091db;
  position: relative;
}
.main .main_middle img{
  left: 2.992rem;
  top: 0.492rem;
  float: left;
  position: absolute;
}
.main .main_bottom {
  width: 400px;
  height: 549px;
  background-color: #ffffff;
  border-radius: 10px;
  box-shadow: 0px 2px 10px 0px 	rgba(0, 0, 0, 0.35);
  position: absolute;
  top: 0.2rem;
  right: 3.083rem;

  float: left;
}
.main .main_bottom .main_top{
  margin-left: 51px;
  height: 18px;
}
.main .main_bottom h2 {
  height: 18px;
  font-family: 'SourceHanSansCN-Regular';
  font-size: 18px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #94c52a;
  position: absolute;
  float: left;
}
.main .main_bottom div {
  width: 100%;
  height: 40px;
  margin-top: 30px;
  margin-left: 50px;
  font-size: 14px;
}
.main .main_bottom div p {
  font-size: 14px;
  color: #bd2424;
  position: absolute;
  margin-top: 45px;
  margin-left: 0px;
}
.main .main_bottom div p:nth-child(4) {
  margin-top: 0px;
}
.main .main_bottom .telepone {
  margin-top: 29px;
}
.main .main_bottom input {
  width: 301px;
  height: 40px;
  text-align: center;
  border-radius: 5px;
  border: solid 1px #bbbbbb;
  position: absolute;
  font-size: 14px;
  z-index: 99!important;
  outline: none;
}
.main .main_bottom div input::-webkit-input-placeholder {
  color: #999999;
}
.main .main_bottom div input::-moz-input-placeholder {
  color: #999999;
}
.main .main_bottom div input:-moz-input-placeholder {
  color: #999999;
}
.main .main_bottom div input::-ms-input-placeholder {
  color: #999999;
}
.main .main_bottom .yanzheng{
  margin-left: 49px;
}
.main .main_bottom .yanzheng input:first-child {
  width: 188px;
  margin-right: -1%;
}
.main .main_bottom .yanzheng input:nth-child(2) {
  display: inline-block;
  width: 95px;
  height: 40px;
  background-color: #94c52a;
  color: #ffffff;
  border-radius: 5px;
  margin-left: 207px;
  border: none;
}
.main .main_bottom .yanzheng input:nth-child(2):hover {
  cursor: pointer;
}
.main .main_bottom .identifying input {
  width: 105px;
  height: 40px;
  border-radius: 5px;
  border: solid 1px #bbbbbb;
  margin-right: -1%;
  outline: none;
}
.main .main_bottom .identifying #canvas {
  margin-left: 120px;
  width: 100px;
  height: 40px;
  border: 1px solid #bbbbbb;
  border-radius: 5px;
}
.main .main_bottom .identifying .un_clear {
  width: 65px;
  height: 41px;
  border-radius: 5px;
  border: solid 1px #bbbbbb;
  margin-left: 14px;
  font-size: 14px;
  background-color: #ffffff;
}
.main .main_bottom .identifying .un_clear {
  color: #94c52a;
}
.main .main_bottom .identifying .un_clear {
  color: #94c52a;
}
.main .main_bottom .identifying .un_clear {
  color: #94c52a;
}
.main .main_bottom .identifying .un_clear {
  color: #94c52a;
}
.main .main_bottom .identifying .un_clear:hover {
  cursor: pointer;
}
.main .main_bottom .agree {
  margin-top: 30px;
  height: 15px;
}
.main .main_bottom .agree input {
  width: 11px;
  height: 12px;
  opacity: 0;
  margin-top: 3px;
}
.main .main_bottom .agree label{
  background-color: white;
  border-radius: 0px;
  border: 1px solid #bbbbbb;
  width: 10px;
  height: 11px;
  display: inline-block;
  text-align: center;
  vertical-align: top;
  line-height: 11px;
  margin-right: -1%;
  margin-top: 3px;

}
.main .main_bottom .agree label:hover {
  cursor: pointer;
  border: 1px solid #bbbbbb;
}
.main .main_bottom .agree .check:hover {
  cursor: pointer;
  border: 1px solid #bbbbbb;
}
.main .main_bottom .agree .check:checked+label{
  background-color: #ffffff;
  background: #ffffff;
}
.main .main_bottom .agree .check:checked+label:after {
  content: "\2713";
  color: #666666;
  border-color: #666666;
}
.main .main_bottom .agree .agree_span {
  width: 255px;
  height: 15px;
  font-family: 'SourceHanSansCN-Regular';
  font-size: 14px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #666666;
  margin-left: 5px;
  line-height: 15px;
  text-decoration: none;
}
.main .main_bottom .agree .agree_span2 {
  text-decoration: none;
  color: #94c52a;
}
.main .main_bottom .register_now {
  border-radius: 5px;
}
.main .main_bottom .register_now input {
  background-color: #94c52a;
  color: #ffffff;
}
.main .main_bottom .register_now .click_register:hover {
  cursor: pointer;
}
.main .main_footer{
  height: 15px;
	font-family: 'SourceHanSansCN-Regular';
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
  color: #666666;
  text-align: center;
  margin-top: 0.833rem;
  margin-bottom: 100px;
}
.error {
  font-size: 14px;
  color: #bd2424;
  margin-top: -10px;
  margin-left: 0px;
}
.error1 {
  font-size: 14px;
  color: #bd2424;
  margin-top: -10px;
  margin-left: 0px;
}
.existing{
  float: right;
  margin-right: 3.1rem;
  margin-top: 60px;
  font-size: 14px;
  color: #666;
}
.existing a{
  color: #247dd0;
  text-decoration: none;
}