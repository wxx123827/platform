.basic_content {
    clear: both;
    width: 100%;
}

.content {
    width: 1200px;
    margin: 0 auto;
}

.content .left {
    width: 190px;
    height: 456px;
    background-color: #ffffff;
    text-align: center;
    margin-top: 10px;
}
.content .left .layui-upload .layui-btn{
    width: 100px!important;
    height: 30px!important;
    line-height: 30px;
	background-color: #eeeeee!important;
	border-radius: 5px;
    border: solid 1px #a0a0a0;
    margin-top: 20px;
    margin-left: 45px;
}
.content .left ul{
    margin-top: 10px;
}
.content .left ul li{
    width: 190px;
    height: 35px;
    line-height: 35px;

}
.content .left ul li:hover{
    background-color: #eeeeee;
}

.content .left ul li a{
    font-size: 16px;
    display: inline-block;
    width: 190px;
    height: 35px;
    line-height: 35px;
    
}
.content .right {
    width: 1000px;
    height: 750px;
    background-color: #ffffff;
    margin-top: 10px;
}

.content .right p {
    float: left;
}
.content .right p span{
    font-size: 14px;
}
.content .right .right_top{
    width: 1000px;
	height: 50px;
    background-color: #f1f8ff;
    line-height: 50px;
}
.content .right .right_top label{
    margin-left: 10px;
}
.content .right .right_top label input{
    display: inline-block;
    width: 120px;
	height: 24px;
	border: solid 1px #999999;
}
.right_top .refer{
    width: 60px;
    height: 24px;
    text-align: center;
    line-height: 24px;
	background-color: #247dd0;
    border-radius: 5px;
    float: right;
    margin-top: 15px;
    font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #fefefe;
}

.list_title{
    padding-left: 20px;
}
.list_title span{
    display: inline-block;
    width: 130px;
}
.shops_name{
    padding-left: 20px;
    width: 1000px;
    height: 30px;
    line-height: 30px;
	background-color: #eeeeee;
}
.shops_content{
    padding-left: 20px;
    height: 120px;
    line-height: 120px;
	border: solid 1px #eeeeee;
}
.shops_content:hover{
    border: solid 1px #aad6ff;
}
.shops_content span{
    display: inline-block;
    width: 130px;
}
.shops_bottom{
    color: #247dd0;
}
.complete{
    float: left;
    margin-left: 20px;
}
.favorite{
    float: right;
}
#favorite{
    float: left;
    margin-right: 30px;
}
#delete{
    float: right;
    margin-right: 20px;
}
.content .right .next button {
    width: 100px!important;
	height: 40px!important;
	/* background-color: #247dd0; */
    border-radius: 5px;
    border: solid 1px #247dd0;
    font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #247dd0;
    margin-right: 10px;
}
.content .right .next button:hover{
    background-color: #247dd0;
    color: #ffffff;
}
.content .right .next .save{
    margin-left: 200px;
}