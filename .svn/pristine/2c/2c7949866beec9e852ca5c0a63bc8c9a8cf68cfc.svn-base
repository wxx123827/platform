* {
  margin: 0;
  padding: 0;
}
@font-face {
  font-family: 'SourceHanSansCN-Regular';
  src: url('../font/SourceHanSansCN-Regular.otf');
  src: url('../font/SourceHanSansCN-Regular.otf') format('otf');
  font-weight: normal;
  font-style: normal;
}
html,
body {
  height: 100%;
  width: 100%;
}
body {
  background: url(../imgs/bg.jpg) no-repeat;
  background-size: 100% 100%;
}
.main {
  position: relative;
  width: 100%;
  height: 100%;
}
.main .main_content {
  width: 400px;
  height: 480px;
  background-color: #ffffff;
  border-radius: 10px;
  position: absolute;
  top: 2.125rem;
  left: 9.58333333rem;
}
.main .main_content .main_text {
  width: 200px;
  height: 60px;
  font-family: 'SourceHanSansCN-Regular';
  font-size: 24px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #333333;
  margin-top: 0.25rem;
  margin-left: 130px;
}
.main .main_content div {
  height: 40px;
  margin-top: 0.25rem;
  margin-left: 50px;
}
.main .main_content div p {
  font-size: 14px;
  color: #bd2424;
  margin-top: -0.08333333rem;
  margin-left: 0px;
  z-index: 1;
}
.main .main_content div input {
  width: 300px;
  height: 40px;
  text-align: center;
  border-radius: 5px;
  border: solid 1px #bfbfbf;
  font-size: 14px;
  position: relative;
  z-index: 99;
}
.main .main_content .telepone {
  margin-top: -0.83333333rem;
  height: 40px;
}
.main .main_content .login {
  margin-bottom: -0.08333333rem;
}
.main .main_content .login input {
  background-color: #62b0ed;
  color: #ffffff;
}
.main .main_content .find_pwd a {
  font-size: 14px;
  text-decoration: none;
}
.main .main_content .forget_pwd {
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #666666;
  margin-left: 165px;
}
.main .main_content .zhuce {
  font-family: 'SourceHanSansCN-Regular';
  font-size: 14px;
  font-weight: normal;
  letter-spacing: 0px;
  color: #62b0ed;
  margin-left: -25px;
}
.main .main_content .others {
  font-family: 'SourceHanSansCN-Regular';
  font-size: 14px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #999999;
  margin-top: 1.33333333rem;
  margin-left: 160px;
}
.main .main_content .others_icon {
  margin-top: -0.75rem;
  margin-left: 145px;
}
.main .main_content .others_icon .qq_icon {
  display: inline-block;
  width: 33px;
  height: 33px;
  background: url('../imgs/qq.png') no-repeat;
}
.main .main_content .others_icon .qq_icon:hover {
  background: url('../imgs/qq_active.png') no-repeat;
}
.main .main_content .others_icon .wx_icon {
  display: inline-block;
  width: 40px;
  height: 33px;
  background: url('../imgs/wx.png') no-repeat;
}
.main .main_content .others_icon .wx_icon:hover {
  background: url('../imgs/wx_active.png') no-repeat;
}
.error {
  font-size: 14px;
  color: #bd2424;
  margin-top: -15px;
  margin-left: 0px;
}
