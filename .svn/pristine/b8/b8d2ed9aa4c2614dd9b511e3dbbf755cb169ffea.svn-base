body{
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    background-color: #f6f6f6;
}
#nav{
    width: 100%;
    height: 100px;
	background-color: #247dd0;
}
#nav .nav_content{
    width: 1200px;
    margin: 0 auto;
   height: 100px;
}
.center_left img{
    margin-top: 30px;
    width: 150px;
}
.center_middle{
    border: none;
    line-height: 100px;
    width: 40%;
}
#search{
    margin-top: 30px;
}
#search input{
    width: 320px;
	height: 45px;
	border-radius: 10px 0px 0px 10px;
    border: solid 2px #eeeeee;
    float: left;
    box-sizing: border-box;
}
#search button{
   float: left;
    width: 120px;
    border: none;
    height: 45px;
    color: #247dd0;
	background-color: #eeeeee;
	border-radius: 0px 10px 10px 0px;
}
.provider_title{
    height: 50px;
    line-height: 50px;
    clear: both;
    border-bottom: 2px solid #247dd0;
}
.provider_title .more{
    float: right;
    color: #247dd0;
    font-size: 20px;
}
.provider_list{
    width: 1200px;
    margin: 0 auto;
}
.content_box img{
    width: 265px;
    height: 200px;
    margin: 10px;
    box-sizing: border-box;
   object-fit: scale-down;
}
.content_box p{
margin-top: 10px;
}
.provider_content div{
    margin-right: 20px;
    float: left;
    width: 285px;
	height: 350px;
    background-color: #ffffff;
    text-align: center;
    margin-top: 10px;
}
.provider_content div:nth-child(4n){
    margin-right: 0px;
}
.title_select{
    background-color: #eeeeee;
}
.title_select a{
    margin-left: 10px;
}
.title_select .classifty{
    min-height: 35px;
    height: auto;
    padding-top: 10px;
    box-sizing: border-box;
}
.title_select .sort{
    min-height: 35px;
    height: auto;
}
#page{
	clear: both;
	width: 1200px;
	margin: 20px auto;
}
#goods_list{
   margin-bottom: 500px;
}
