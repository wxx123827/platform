.basic_content {
    clear: both;
    width: 100%;
}

.content {
    width: 1200px;
    margin: 0 auto;
}

.content .left {
    width: 190px;
    height: 456px;
    background-color: #ffffff;
    text-align: center;
    margin-top: 10px;
}
.content .left .layui-upload .layui-btn{
    width: 100px!important;
    height: 30px!important;
    line-height: 30px;
	background-color: #eeeeee!important;
	border-radius: 5px;
    border: solid 1px #a0a0a0;
    margin-top: 20px;
    margin-left: 45px;
}
.content .left ul{
    margin-top: 10px;
}
.content .left ul li{
    width: 190px;
    height: 35px;
    line-height: 35px;

}
.content .left ul li:hover{
    background-color: #eeeeee;
}

.content .left ul li a{
    font-size: 16px;
    display: inline-block;
    width: 190px;
    height: 35px;
    line-height: 35px;
    
}
.content .right {
    width: 1000px;
    background-color: #ffffff;
    margin-top: 10px;
}

.content .right p {
    float: left;
    height: 130px;
}
.content .right p span{
    font-size: 14px;
}
.content .right .right_top{
    width: 1000px;
	height: 50px;
    background-color: #f1f8ff;
    line-height: 50px;
    border-bottom: 1px solid #999999;
}
.content .right .right_top label{
    margin-left: 10px;
}
.content .right .right_top label input{
    display: inline-block;
    width: 120px;
	height: 24px;
	border: solid 1px #999999;
}
.right_top .refer{
    width: 60px;
    height: 24px;
    text-align: center;
    line-height: 24px;
	background-color: #247dd0;
    border-radius: 5px;
    float: right;
    margin-top: 15px;
    font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #fefefe;
    margin-right: 15px;
}

.list_title{
    padding-left: 20px;
    height: 40px;
    line-height: 40px;
}
.list_title span:first-child{
    width: auto;
    margin-left: 20px;
}
.list_title span{
    width: 135px;
    display: inline-block;
    text-align: center;
    font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
}
.layui-form-item .layui-form-checkbox[lay-skin=primary]{
    margin-top: 7px!important;
}
.shops_name{
    padding-left: 20px;
    height: 30px;
    background-color: #eeeeee;
    font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #666666;
}
.shops_content{
    padding-left: 20px;
    height: 120px;
    line-height: 120px;
    border: solid 1px #eeeeee;
    border-top: 1px solid #fff;
}
.shops_content:hover{
    border: solid 1px #aad6ff;
}
.shops_content span{
    width: 135px;
    display: inline-block;
    text-align: center;
    font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #666666;
}
.shops_content .goods_name{
   width: auto;
   display: inline-block;
   min-width: 140px;
}
.shops_content img{
    vertical-align: middle;
}
.shops_bottom{
    color: #247dd0;
    height: 40px;
    background-color: #f1f8ff;
    line-height: 40px;
    margin-top: 10px;
}
.complete{
    float: left;
    margin-left: 20px;
    cursor: pointer;
}
.favorite{
    float: right;
}
#favorite{
    float: left;
    margin-right: 30px;
    cursor: pointer;
}
#delete{
    float: right;
    margin-right: 20px;
    cursor: pointer;
}
.content .right .next button {
    width: 100px!important;
	height: 40px!important;
	/* background-color: #247dd0; */
    border-radius: 5px;
    border: solid 1px #247dd0;
    font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #247dd0;
    margin-right: 10px;
}
.content .right .next button:hover{
    background-color: #247dd0;
    color: #ffffff;
}
.content .right .next .save{
    margin-left:350px;
}