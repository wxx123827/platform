* {
  margin: 0;
  padding: 0;
}
@font-face {
  font-family: 'SourceHanSansCN-Regular';
  src: url('../font/SourceHanSansCN-Regular.otf');
  src: url('../font/SourceHanSansCN-Regular.otf') format('otf');
  font-weight: normal;
  font-style: normal;
}
html,
body {
  height: 100%;
  width: 100%;
  min-width: 1200px;
}
body {
  background-size: 100% 100%;
}
.main {
  position: relative;
  width: 100%;
  height: 100%;
}
.main_top{
  width: 100%;
  height: 100px;
}
.main .main_top img{
  margin-top: 0.183rem;
  margin-bottom: 1rem;  
  margin-left: 3rem;
  width: 200px;
  height: 56px;
}
.main_middle{
  width: 100%;
  height: 600px;
  background-color: #0091db;
  position: relative;

}
.main .main_middle img{
  width: 581px;
  height: 482px;
  margin-left: 2.992rem;
  margin-top: 0.492rem;
  float: left;
}
.main .main_content {
  width: 400px;
  height: 478px;
  background-color: #ffffff;
  border-radius: 10px;
  box-shadow: 0px 2px 10px 0px 	rgba(0, 0, 0, 0.35);
  position: absolute;
  top: 0.508rem;
  right: 3.083rem;
  float: left;
}
.main .main_content .main_text {
	height: 18px;
	font-family: 'SourceHanSansCN-Regular';
	font-size: 18px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
  color: #94c52a;
  margin-top: 37px;
  margin-left: 49px;
  line-height: 18px;
}
.main .main_content div {
  height: 40px;
  margin-left: 49px;
  margin-top: 30px;
}
.main .main_middle .main_content div p {
  font-size: 14px;
  height: 14px;
  color: #bd2424;
  position: absolute;
  margin-top: 45px;
  margin-left: 0px;
}
.main .main_content div input {
  width: 301px;
  height: 40px;
  text-align: center;
  border-radius: 5px;
  border: solid 1px #94c52a;
  font-size: 14px; 
  font-family: 'SourceHanSansCN-Regular';
  position: absolute;
  z-index: 99;
  font-weight: normal;
	font-stretch: normal;
  letter-spacing: 0px;
}
.main .main_content div input::-webkit-input-placeholder {
  color: #999999;
  font-size: 14px;
}
.main .main_content div input::-moz-input-placeholder {
  color: #999999;
  font-size: 14px;
}
.main .main_content div input:-moz-input-placeholder {
  color: #999999;
  font-size: 14px;
}
.main .main_content div input::-ms-input-placeholder {
  color: #999999;
  font-size: 14px;
}
.main .main_content .telepone{
  margin-top: 29px;
  margin-left: 49px;
}
.main .main_content .login input {
  background-color: #94c52a;
  color: #ffffff;
}
.main .main_content .find_pwd{
  width: 118px;
  height: 15px;
  margin-left: 232px;
  margin-top: 16px;
  line-height: 15px;
  font-family: 'SourceHanSansCN-Regular';
}
.main .main_content .find_pwd a {
  font-size: 14px;
  text-decoration: none;
  position: absolute;
}
.main .main_content .forget_pwd {
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #94c52a;
}
.main .main_content .register{
  margin-left: 62px;
  color: #666666;
}
.main .main_content .others {
  width: 300px;
  height: 15px;
  text-align: center;
  font-family: 'SourceHanSansCN-Regular';
  font-size: 14px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #999999;
  margin-top: 74px;
  z-index: 99;
  line-height: 15px;
  position: absolute;
}
.main .main_content .others:after, .main .main_content .others:before {
  position: absolute;
  top: 50%;
  background: #cccccc;
  content: "";
  height: 1px;
  width: 35%;
}
.main .main_content .others:before {
  left: 0;
}
.main .main_content .others:after {
  right: 0;
}
.main .main_content .others_icon {
  /* margin-top: 30px; */
  margin-top: 119px;
  margin-left: 148px;
}
.main .main_content .others_icon .qq_icon {
  display: inline-block;
  position: absolute;
  width: 31px;
  height: 33px;
  background: url('../imgs/qq.png') no-repeat;
}
.main .main_content .others_icon .qq_icon:hover {
  background: url('../imgs/qq_active.png') no-repeat;
}
.main .main_content .others_icon .wx_icon {
  position: absolute;
  margin-left: 0.542rem;
  display: inline-block;
  width: 40px;
  height: 33px;
  background: url('../imgs/wx.png') no-repeat;
}
.main .main_content .others_icon .wx_icon:hover {
  background: url('../imgs/wx_active.png') no-repeat;
}
.main .main_bottom{
	height: 15px;
	font-family: 'SourceHanSansCN-Regular';
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
  color: #666666;
  text-align: center;

  margin-top: 0.833rem;
}
.error {
  font-size: 14px;
  color: #bd2424;
  margin-top: -15px;
  margin-left: 0px;
}
