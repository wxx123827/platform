.basic_content {
    clear: both;
    width: 100%;
}

.content {
    width: 1200px;
    margin: 0 auto;
}

.content .left{
    width: 190px;
    height: 456px;
    background-color: #ffffff;
    text-align: center;
}
.content .left ul{
    margin-top: 20px;
}
.content .left li a{
    font-size: 16px;
    display: inline-block;
    width: 190px;
    height: 35px;
    line-height: 35px;
}
.content .left li{
    width: 190px;
    height: 35px;
    line-height: 35px;
}
.content .left li:hover{
    width: 190px;
    height: 35px;
    background-color: #eeeeee;
}

.content .right {
    width: 1000px;
    background-color: #ffffff;
    padding-bottom: 120px;
}

.content .right h2{
    font-size: 20px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
    color: #333333;
    padding: 20px 0px 10px 20px;
}
.bottom{
    height: 40px;
    margin-top: 50px;
}
.bottom .save{
    margin-left:30%!important;
    
}
.bottom .save,.bottom .cancel{
    width: 100px;
    float: left;
    height: 40px;
    line-height: 40px;
    text-align: center;
	border-radius: 5px;
    border: solid 1px #247dd0;
    color: #247dd0;
    font-size: 14px;
    margin-left: 20px;
}
.bottom .save:hover,.bottom .cancel:hover{
    background-color: #247dd0;
    color: #ffffff;
}