body {
	padding: 0;
	margin: 0 auto;
	background-color: #f9f9f9;
}

* {
	padding: 0;
	margin: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	font-family: 'Microsoft yahei', "Consolas", "Monaco", "Bitstream Vera Sans Mono", "Courier New", Courier, monospace;
	letter-spacing: 1px;
}

@font-face {
	font-family: 'SourceHanSansCN-Normal';
	src: url('../font/SourceHanSansCN-Normal.otf');
	src: url('../font/SourceHanSansCN-Normal.otf') format('otf');
	font-family: 'SourceHanSansCN-Bold';
	src: url('../font/SourceHanSansCN-Bold.otf');
	src: url('../font/SourceHanSansCN-Bold.otf') format('otf');
	font-weight: normal;
	font-style: normal;
}


.container {
	width: 1200px;
	margin: 0 auto;
	clear: both;
}

.fl {
	float: left;
}

.index {
	margin-left: 100px;
}

.fr {
	float: right;
}



.clear,
.clearfix {
	clear: both;
}



a {
	text-decoration: none;
	color: #666;
}

a:link,
a:visited,
a:active {
	text-decoration: none;
}

#sorts {
	width: 200px;
	background-color: #ffffff;
	height: 420px;
}

#sorts li {
	list-style: none;
}


#nav_type {
	height: 38px;
	width: 100%;
	margin-top: 20px;
	min-width: 1200px;
	position: relative;
	z-index: 6;
}

#nav_type .nav {
	/* padding: 0 18px; */
	height: 38px;
	width: 770px;
	background-color: #fff;
	white-space: nowrap;
}

#nav_type .nav li {
	list-style: none;
}

#nav_type .nav li.fl div.nav_item {
	padding: 11px;
	margin-right: 20px;
	position: relative;
}

#nav_type .nav li.fl div.nav_item>a,
#nav_type .nav li.fl div.nav_item>a:visited {
	display: block;
	padding: 0 18px;
	font-size: 16px;
	line-height: 1;
	font-family: 'SourceHanSansCN-Bold';
	color: #333;
	cursor: pointer;
}

#nav_type .nav li.fl div.nav_item>a.bold,
#nav_type .nav li.fl div.nav_item>a.active,
#nav_type .nav li.fl div.nav_item>a:visited.bold {
	font-weight: bold;
}

#nav_type .nav li.fl div.nav_item>a.border,
#nav_type .nav li.fl div.nav_item>a:visited.border {
	border-right: 1px solid #ddd;
}

#nav_type .nav li.fl div.nav_item>a.active,
#nav_type .nav li.fl div.nav_item>a:visited.active {
	color: #247dd0;
}

#nav_type .nav li.fl div.nav_item>a:hover:not(.active),
#nav_type .nav li.fl div.nav_item>a:visited:hover:not(.active) {
	color: #247dd0;
}

#drop_down_nav .trangel {
	display: none;
	position: absolute;
	border: 6px solid transparent;
	border-bottom-color: #272c2d;
	bottom: 0;
	margin-left: -3px;
	z-index: 0;
	left: 445px;
}

#drop_down_nav li.fl:hover .dropdown_list_container {
	display: block;
	-webkit-transition: all 0.2s ease-in-out 0.2s;
	-moz-transition: all 0.2s ease-in-out 0.2s;
	-ms-transition: all 0.2s ease-in-out 0.2s;
	-o-transition: all 0.2s ease-in-out 0.2s;
	transition: all 0.2s ease-in-out 0.2s;
}

#drop_down_nav .dropdown_list_container {
	top: 32px;
	position: absolute;
	width: 100%;
	-webkit-transition: all 0.2s ease-in-out;
	-moz-transition: all 0.2s ease-in-out;
	-ms-transition: all 0.2s ease-in-out;
	-o-transition: all 0.2s ease-in-out;
	transition: all 0.2s ease-in-out;
	left: 0;
	height: 0;
	overflow: hidden;
	display: none;
	z-index: 16;
	padding-top: 6px;
}

#drop_down_nav .dropdown_list_container div.bg {
	background: #272c2d;
}

#drop_down_nav .dropdown_list_container .dropdown_list_item {
	position: relative;
	padding: 28px 0;
	width: 1200px;
	margin: 0 auto;
}

#drop_down_nav .dropdown_list_container .dropdown_list {
	display: inline-block;
	position: relative;
}

#drop_down_nav .dropdown_list_container dl {
	display: inline-block;

	text-align: left;
	vertical-align: top;
	margin: 0 8px;
}

#drop_down_nav .dropdown_list_container dl dt {
	font-size: 14px;
	font-weight: 600;
	height: 24px;
	line-height: 24px;
	color: #fff;
	margin-bottom: 12px;
}

#drop_down_nav .dropdown_list_container dl dd {
	line-height: 24px;
}

#drop_down_nav .dropdown_list_container dl dd a {
	font-size: 12px;
	line-height: 24px;
	color: #a3a8a9;
}

#drop_down_nav .dropdown_list_container dl dd a>b.hot {
	position: relative;
	display: block;
	width: 30px;
	float: right;
	height: 14px;

	right: -6px;
	top: 0;
}

#drop_down_nav .dropdown_list_container dl dd:hover a {
	color: #f90;
}

#all_types {
	width: 16.66666667%;
	padding-left: 16px;
	line-height: 38px;
	background-color: #eeeeee;
	color: #333333;
	font-size: 14px;
}

#all_types:before {
	width: 16px;
	height: 16px;
	content: '';
	display: inline-block;
	position: relative;
	top: -2px;
	margin-right: 12px;
	vertical-align: middle;

}

#banner_types {
	height: 442px;
	margin-bottom: 10px;
	position: relative;
}

#sorts {
	position: relative;
}

#sorts ul {
	margin-top: 20px;
	margin-left: 40px;
}

#sorts ul h3 {
	font-size: 18px;
}

#sorts li {
	cursor: pointer;
	padding-top: 5px;
	font-size: 16px;
	box-sizing: border-box;
}

#sorts li:hover {
	color: #247dd0;
}

.tabs {
	position: relative;
}


#sorts div.child {
	display: none;
	position: absolute;
	top: 10px;
	max-height: 442px;
	width: 770px;
	z-index: 16;
	color: #272525;
	overflow: hidden;
	left: 160px;
	padding: 20px 10px;
	border: 1px solid #f7f7f7;
	background: #fff;
	-webkit-box-shadow: 2px 0 5px rgba(0, 0, 0, 0.3);
	-moz-box-shadow: 2px 0 5px rgba(0, 0, 0, 0.3);
	box-shadow: 2px 0 5px rgba(0, 0, 0, 0.3);
}

#sorts li:hover div.child {
	display: block;
}

#sorts div.child dd {
	padding-left: 15px;
	position: relative;
	min-height: 22px;
}


#sorts div.child li dt a {
	color: #666;
	font-size: 12px;
	font-weight: bold;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	display: inline-block;
	max-width: 80%;
	line-height: 20px;
}


#sorts div.child li:hover {
	color: #f60;
}

#sorts div.child li dt a:hover:after {
	color: #f60;
}

#sorts div.child li dd span {
	font-size: 12px;
	float: left;
	margin: 4px 0;
	padding: 0 10px;
	height: 16px;
	border-left: 1px solid #e0e0e0;
	line-height: 16px;
	white-space: nowrap;
}

#sorts div.child li dd span a:hover {
	color: #f60;
}

#banners {
	width: 83.33333333%;
	height: 100%;
}

#banners .carousel_container {
	width: 75.75757576%;
}

#banners .main_banner {
	width: 100%;
	height: 430px;
}

#banner_right {
	position: relative;
	width: 24%;
	height: 460px;
	margin-top: -40px;
	background-color: #ffffff;
