.basic_content {
    clear: both;
    width: 100%;
}

.content {
    width: 1200px;
    margin: 0 auto;
}

.content .left {
    width: 190px;
    height: 456px;
    background-color: #ffffff;
    text-align: center;
}

.content .left ul {
    margin-top: 20px;
}

.content .left li a {
    font-size: 16px;
    display: inline-block;
    width: 190px;
    height: 35px;
    line-height: 35px;
}

.content .left li {
    width: 190px;
    height: 35px;
    line-height: 35px;
}

.content .left li:hover {
    width: 190px;
    height: 35px;
    background-color: #eeeeee;
}

.content .rights {
    width: 1000px;
    background-color: #ffffff;
    padding-bottom: 120px;
    float: right;
}

.content .rights h2 {
    font-size: 20px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #333333;
    padding: 20px 0px 10px 20px;
}

.right_content form p {
    margin-top: 20px;
}

.right_content form p span {
    color: #333333;
}

.right_content form p input {
    height: 30px;
    width: 870px;
    background-color: #ffffff;
    border: 1px solid rgb(169, 169, 169);
}

.right_content form .imgs_one {
    margin-left: 100px;
    margin-right: 15px;
    margin-top: -20px;
}

.right_content form p textarea {
    position: absolute;
    left: 75px;
    width: 870px;
    min-height: 100px;
    height: auto;
    resize: none;
    background-color: #ffffff;
}

.detail_submit .alter {
    width: 200px;
    height: 40px;
    line-height: 40px;
    background-color: #247dd0;
    border-radius: 5px;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #ffffff;
}